@import 'fonts'

@media (max-width: 1660px) and (min-width: 1281px)

    #instagram
        .image_wrapper
            .image
                img
                    width: 100%
                    height: 412px

    .form_button
        &.alt
            .default_btn
                padding: 10px 30px

    .form_flex
        &.with_btn
            &.alt_2
                .form_button
                    .default_btn
                        padding: 10px

    /**
     * Login/Sign up */
    .login_sign_up
        .close_icon
            top: calc(0% + 15px)
        &.front,
        &.not_front
            margin-top: 60px
            &.sticky
                margin-top: 60px
        #login
            height: calc(100% - 150px)
            overflow-y: scroll
            overflow-x: hidden
            padding: 0 30px
            max-width: 40%
            #default_form,
            .new_here
                margin-top: 30px
            .new_here
                neutra_book(14px, false)
                font-weight: 500
            .action
                .default_btn_login
                    margin: 30px 0
                    span
                        neutra_book(14px, false)
                        font-weight: 500
            .title
                neutra_bold(18px, false)
    /**
     * Main */
    #main_container
        #complete_profile
            top: calc(0% + 60px)
            padding: 5px 10px
        #header
            &.instructor,
            &.not_front,
            &.front
                .navbar
                    padding: 10px 10px 10px 30px
            .navbar
                flex: 1 0 70%
                .logo
                    img
                        width: 30px
                    .logo_title
                        neutra_bold(16px, true)
                        padding-left: 10px
                .nav_list
                    li
                        margin: 0 20px
                        .nav_item
                            neutra_bold(12px, true)
                .user_dropdown
                    .user_dropdown_list
                        width: 220px
                        top: calc(100% + 15px)
                        .user_dropdown_item
                            .item_link
                                neutra_book(14px, false)
                                padding: 10px 15px
                    h3
                        neutra_bold(14px, false)
                        margin-left: 10px
            .nav_burger
                flex: 0 0 35px
                margin: 0 10px
                .bar
                    &.bottom_line
                        width: calc(100% - 15px)
        #navbar_expanded
            min-height: 200px
            overflow-y: scroll
            overflow-x: hidden
            &.sticky
                .logo
                    padding: 10px 30px
                .close_icon
                    top: calc(0% + 10px)
                    right: 0
            .close_icon
                top: calc(0% + 10px)
                right: 0
            .logo
                padding: 10px 30px
                img
                    width: 30px
                .logo_title
                    neutra_bold(16px, true)
                    padding-left: 10px
                    letter-spacing: 3px
            .content
                padding: 20px 30px
                .left
                    margin-bottom: 30px
                    .expanded_main_list
                        li
                            margin-bottom: 40px
                            &:last-child
                                margin-bottom: 0
                            .main_item
                                &.nuxt-link-exact-active,
                                &.nuxt-link-active,
                                &:hover,
                                &:focus
                                    &::before
                                        right: calc(0% - 35px)
                                #menu
                                    width: auto
                .right
                    .expanded_nav_list
                        li
                            margin-bottom: 20px
                            .nav_item
                                neutra_bold(16px, true)
                    .expanded_nav_description
                        padding-top: 10px
                        p
                            neutra_book(16px, false)
                            a
                                neutra_bold(16px, false)
                        .link
                            margin-bottom: 20px
                            p
                                neutra_book(16px, false)
                            span
                                neutra_bold(16px, false)
                        .social_links
                            margin-top: 20px
                            h4
                                neutra_book(16px, false)
                            a
                                margin: 0 10px
                                width: 20px
                                height: 20px
                .left,
                .right
                    flex: 0 0 45%
        #footer
            padding: 40px
            .footer_link
                &.alt
                    p
                        neutra_book(14px, false)
                        a
                            neutra_bold(14px, false)
                h3
                    neutra_book(14px, false)
                    font-weight: 500
                ul
                    li
                        .footer_item
                            neutra_book(14px, false)
                .social_links
                    h4
                        neutra_book(14px, false)
                        font-weight: 500
            .back_to_top
                right: calc(0% - 40px)
        #pro_tip
            &.adjust
                top: calc(0% + 110px)
            top: calc(0% + 61px)
            padding: 5px 30px
            .tip
                b,
                strong
                    neutra_bold(14px, false)
                p,
                a,
                span
                    neutra_book(14px, false)
    /**
     * Home */
    .home
        #reviews
            .header
                margin: 0
                .description
                    p
                        strong
                            neutra_bold(22px, true)
            .content
                .review_slide
                    img
                        height: 50px
                    .description
                        neutra_book(14px, false)
        #instructors
            padding: 25em 0
            .overlay
                .info
                    &::before
                        background-size: contain
                    .title
                        neutra_bold(22px, false)
                    .description
                        neutra_book(16px, false)
                        margin: 30px 0
        #studios
            .header
                padding: 30px 0
                .description
                    margin-right: 40px
                    p
                        strong
                            neutra_bold(22px, true)
            .content
                .studio_content
                    max-width: calc(80%)
                    .wrapper
                        padding-bottom: 60px
                        .right
                            .overlay
                                padding: 40px 30px
                                .content_select
                                    .input
                                        .select
                                            select
                                                width: auto
                                .content_flex
                                    .title
                                        neutra_book(30px, false)
                                        font-weight: 500
                                        margin-top: 15px
                                    .description
                                        margin: 20px 0
                                        label
                                            neutra_book(16px, true)
                                        a
                                            neutra_book(14px, false)
                                        .opening
                                            p
                                                neutra_book(14px, false)
                                    .default_btn
                                        margin-top: 20px
        #lets_ride
            .header
                padding: 30px 0
                .description
                    p
                        span
                            neutra_bold(24px, false)
    /**
     * Buy Rides */
    .buy_rides
        &.inner
            #payments
                max-width: calc(85%)
                &.dismissed,
                &.dismiss
                    padding: 40px 0 60px
                .step
                    .header_title
                        neutra_bold(20px, false)
                        margin-bottom: 30px
                    .wrapper
                        .left
                            padding-right: 40px
                            .header
                                h2
                                    neutra_book(16px, false)
                            .content
                                ul
                                    li
                                        neutra_book(12px, false)
                        .right
                            padding-left: 40px
                            .breakdown_list
                                .item
                                    p
                                        neutra_book(14px, false)
                                .total
                                    margin: 30px 0 0
                                    p
                                        neutra_bold(16px, false)
                            .breakdown_actions
                                margin: 30px 0 20px
                    .preview_payment
                        .preview
                            .available
                                .available_item
                                    h3
                                        neutra_book(16px, false)
                                    p
                                        neutra_book(16px, false)
                                        font-weight: 500
                                    .unavailable
                                        a
                                            neutra_book(12px, false)
                                            font-weight: 500

                            .item
                                h3,
                                p
                                    neutra_book(16px, false)
                            .total
                                margin: 30px 0
                                p
                                    neutra_bold(16px, false)
        #digital
            .overlay
                h2
                    neutra_bold(30px, false)
                h3
                    neutra_book(16px, false)
    /**
     * Book a bike */
    .book_a_bike
        &.inner
            #content
                max-width: calc(100% - 60px)
                padding: 40px 0
                .step
                    &.form
                        .preview_book_form
                            max-width: 50%
                            .header_title
                                neutra_bold(20px, false)
                                margin-bottom: 20px
                            .preview
                                .item
                                    padding: 10px 0
                                    h3,
                                    p
                                        neutra_book(16px, false)
                                .total
                                    margin: 20px 0
                                    p
                                        neutra_bold(16px, false)
                .main_right
                    .header
                        padding: 20px 10px
                    .content
                        padding: 10px 0
                        .seat_wrapper
                            max-width: 800px
                            .overlay_header
                                top: calc(0% + 40px)
                                h3
                                    neutra_book(20px, false)
                                h4
                                    margin: 5px 0 20px
                                img
                                    width: 125px
                                    height: 125px
                            .overlay_seat
                                &.left
                                    left: calc(0% + 25px)
                                &.right
                                    right: calc(0% + 58px)
                                &.bottom,
                                &.bottom_alt,
                                &.bottom_alt_2
                                    .seat
                                        flex: 0 0 calc(10% - 25px)
                                        margin: 11px
                        .footer
                            .actions
                                padding: 20px 40px
                                margin-top: 20px
                .main_left
                    .header
                        padding: 10px
                        .date
                            neutra_bold(18px, true)
                    .content
                        ul
                            padding: 20px 10px
                            li
                                neutra_book(14px, false)
                    .preview
                        padding: 20px 10px
                        .preview_title
                            neutra_bold(16px, false)
                            margin-bottom: 10px
                        .item
                            p,
                            h3
                                neutra_book(14px, false)
                        .total
                            p
                                neutra_bold(14px, false)
                    .description
                        padding: 0 10px 20px
                        *
                            neutra_book(14px, false)
                        h3
                            neutra_book(16px, false)
                            margin-bottom: 10px
                        ul
                            li
                                neutra_book(14px, false)
                                b
                                    neutra_bold(14px, false)
        &.landing
            #content
                .left
                    .filter
                        .wrapper
                            &.studio_filter,
                            &.view_filter
                                .group
                                    label
                                        neutra_book(14px, false)
                                        font-weight: 500
                            &.instructor_filter
                                .autocomplete
                                    &::before
                                        top: calc(0% + (42px / 2))
                                    .text
                                        neutra_book(14px, false)
                                        padding: 10px 35px
                                    .autocomplete_dropdown
                                        min-height: 300px
                                        .list
                                            img
                                                width: 30px
                                                height: 30px
                                            p
                                                neutra_book(16px, false)
                            h3
                                neutra_book(14px, true)
    /**
     * Instructors */
    .instructors
        #instructors_nav
            .left
                .select
                    .label
                        neutra_bold(16px, false)
                    .custom_select
                        .check
                            label
                                neutra_book(14px, false)
                                font-weight: 500
                                padding: 15px 20px
                                span
                                    padding-left: 15px
                                &.label
                                    padding: 15px 20px
                                    img
                                        height: 25px
            .right
                .input
                    input
                        width: 200px
                        neutra_book(16px, false)
            .left,
            .right
                padding: 10px
        #instructors_content
            .header
                padding: 40px 0 30px
                h1
                    neutra_bold(40px, true)
                .subheader
                    h2
                        neutra_book(16px, false)
                    .select
                        select
                            neutra_book(16px, false)
            .content
                padding: 0 20px
                margin-bottom: 30px
                .wrapper
                    .overlay
                        .title
                            neutra_bold(14px, true)
                            padding: 0 10px
                        .action
                            padding: 20px 10px 10px
                .load
                    margin-top: 30px
    .instructor_inner
        #info
            .left
                padding: 0 50px 0 30px
                h1
                    neutra_bold(40px, true)
                .specializations
                    margin: 20px 0
                    .wrapper
                        h3
                            neutra_book(12px, true)
                            font-weight: 500
                .description
                    margin-bottom: 20px
                    h2
                        neutra_bold(24px, false)
                        margin-bottom: 20px
                    p
                        neutra_book(14px, false)
                .talents
                    margin-bottom: 20px
                    p
                        neutra_book(18px, false)
                .quote
                    padding: 0 0 0 40px
                    p
                        neutra_book(14px, false)
        #classes
            padding-bottom: 50px
            .header
                padding: 30px 0 20px
                h2
                    neutra_bold(35px, true)
                h3
                    neutra_book(20px, false)
            .content
                margin: 10px auto 0
                max-width: 75%
                .wrapper
                    padding: 20px 30px
                    .start
                        .date,
                        .time
                            neutra_bold(16px, false)
                    .info
                        .description
                            h2
                                neutra_bold(16px, false)
        #comments
            margin: 30px auto
            max-width: 75%
            .header
                h2
                    neutra_bold(20px, true)
            .content
                .comment
                    padding: 30px 0
                    .comment_info
                        .comment_header
                            .date
                                neutra_book(14px, false)
                        .title
                            neutra_bold(18px, false)
                        .description
                            neutra_book(14px, false)
            .load_more
                .load
                    margin: 30px auto 0
        #ride_with
            .header
                padding: 30px 0
                h2
                    neutra_bold(35px, true)
                .description
                    span
                        neutra_bold(20px, false)
        #spotify
            .left
                padding: 60px 0 0 60px
                h2
                    neutra_bold(20px, false)
                    margin: 30px auto
            .right
                iframe
                    height: 400px
    /**
     * Studios */
    .studios
        #teaser
            padding: 30px 0
            max-width: 75%
            p
                neutra_book(14px, false)
        #visit
            padding: 30px 0
            h2
                neutra_bold(40px, true)
            .content
                .wrapper
                    h2
                        neutra_bold(16px, true)
        #studio_ig
            .header
                .description
                    span
                        neutra_bold(24px, false)
    .studio_inner
        #images
            .wrapper
                .field_image
                    img
                        height: 350px
    /**
     * Riders Guide */
    .riders_guide
        #teaser
            padding: 40px 0
            h2
                neutra_bold(35px, true)
            p
                neutra_book(16px, false)
        #ready
            padding: 40px
            .header
                h2
                    neutra_bold(35px, true)
                h3
                    neutra_book(16px, false)
            .content
                .wrapper
                    h2
                        neutra_book(18px, true)
                        font-weight: 500
                    p
                        neutra_book(16px, false)
                        font-weight: 300
            .instructors
                .instructor_content
                    .wrapper
                        .overlay
                            .title
                                neutra_bold(14px, true)
                                padding: 0 10px
                            .action
                                padding: 20px 10px 10px
                    .view_all
                        span
                            neutra_bold(14px, true)
        #getset
            .header
                padding: 40px 0 30px
                h2
                    neutra_bold(35px, true)
                p
                    neutra_book(16px, false)
            .content
                padding-bottom: 40px
                .steps
                    .wrapper
                        h2
                            neutra_book(18px, false)
                            b
                                neutra_bold(18px, false)
        #amenities
            .amenities
                padding: 80px 40px
                h2
                    neutra_bold(35px, true)
                .content
                    .wrapper
                        .field_image
                            .overlay
                                neutra_bold(18px, true)
        #ride_info
            .header
                padding: 40px 0 0
                h2
                    neutra_bold(35px, true)
            .content
                padding-bottom: 40px
                max-width: calc(75% + 60px)
                .wrapper
                    .info
                        .count
                            neutra_bold(70px, false)
                        .text
                            neutra_book(24px, true)
                            font-weight: 300
    /**
     * About */
    .about
        #additional
            padding: 40px 0
    /**
     * Ride Rewards */
    .ride_rewards
        #content
            padding: 40px 0
            .top
                max-width: 75%
                .title
                    neutra_bold(35px, true)
                .description
                    neutra_book(16px, false)
            .bottom
                margin: 40px auto 0
                .header_title
                    neutra_bold(35px, true)
        #rewards_levels
            padding: 40px 0
            .main
                .title
                    neutra_bold(35px, true)
                .description
                    neutra_book(16px, false)
    /**
     * News */
    .news
        #content
            margin: 40px auto
            max-width: calc(90% + 60px)
            .news_list
                .bottom
                    .title
                        neutra_bold(16px, true)
                    .date,
                    .description
                        neutra_book(14px, false)
                        font-weight: 300
        &.inner
            #content
                margin: 40px auto
                max-width: calc(90% + 40px)
            #more_things
                h3
                    neutra_bold(35px, true)
                &.desktop
                    margin: 40px auto
                    max-width: calc(90% + 60px)
                    .news_list
                        .bottom
                            .title
                                neutra_bold(16px, true)
                            .date,
                            .description
                                neutra_book(14px, false)
                                font-weight: 300
    /**
     * FAQS */
    .faqs
        #content
            .top
                padding: 40px 0
                max-width: calc(65% + 60px)
                .header_title
                    neutra_bold(35px, true)
                .icons
                    .wrapper
                        .title
                            neutra_book(16px, false)
            .bottom
                .faqs_list
                    max-width: 65%
                    padding: 40px 0
                    .wrapper
                        .title
                            neutra_bold(18px, true)
                        .description
                            b
                                neutra_book(16px, true)
                                font-weight: 500
                            p
                                neutra_book(16px, false)
                                font-weight: 300
                            ul
                                li
                                    neutra_book(16px, false)
                                    font-weight: 300
    /**
     * Contact Us */
    .contact_us
        #content
            .top
                padding: 40px 0
                max-width: 80%
                .right
                    .inquiry_list
                        .wrapper
                            margin-bottom: 20px
                            .title
                                neutra_bold(16px, false)
                                margin-bottom: 10px
                            .link
                                a
                                    neutra_book(16px, false)
                    .connect_title
                        neutra_bold(16px, true)
            .bottom
                .left
                    .gmap
                        height: 500px !important
                .right
                    .studio_item
                        padding: 10px 0
                        .title
                            neutra_book(16px, true)
                            font-weight: 500
                        .info
                            .description
                                neutra_book(14px, false)
                                font-weight: 500
                            .link
                                a
                                    neutra_book(14px, false)
    /**
     * My Profile */
    .my_profile
        #content
            .profile_tab
                max-width: calc(100% - 60px)
                .top
                    .right
                        .data
                            .name
                                neutra_bold(26px, false)
                            .info
                                .label
                                    neutra_book(14px, false)
                .bottom
                    .tab_wrapper
                        .tab_item
                            neutra_book(16px, false)
                            margin-right: 20px
            .profile_tab_content
                .wrapper
                    &.class
                        #default_menu
                            .menu_tab_content
                                max-width: calc(100% - 30px)
                    &.default
                        max-width: calc(100% - 60px)
                    .profile_transactions
                        padding: 20px
                    #default_menu
                        .menu_tab
                            max-width: calc(100% - 60px)
                        .menu_tab_content
                            .profile_classes
                                .class_wrapper
                                    .top
                                        .right
                                            .title
                                                neutra_bold(22px, false)
                                        .left
                                            flex: 0 0 50px
                                            width: 50px
                                            height: 50px
                                            .date
                                                .day
                                                    neutra_bold(22px, false)
                            .profile_packages
                                max-width: calc(100% - 60px)
                    .tab_content_header
                        max-width: calc(100% - 60px)
                        &.alt
                            h2
                                neutra_bold(16px, true)
                            .total
                                neutra_book(16px, false)
                                .count
                                    neutra_bold(16px, false)
                        &.alt3
                            .tab_content_header_menu
                                .header_menu_tab_item
                                    neutra_bold(14px, true)
                        .with_info
                            h2
                                neutra_bold(16px, true)
                    .tab_content_main
                        max-width: calc(100% - 30px)
                        .profile_gift_cards
                            .gift_card_wrapper
                                .bottom
                                    .package_info
                                        .name
                                            neutra_book(18px, false)
                                            font-weight: 500
                    .profile_journey
                        .teaser
                            max-width: calc(100% - 60px)
                            .taken
                                .summary
                                    .summary_content
                                        .class_stat
                                            .class_count
                                                neutra_bold(40px, true)
                                                font-weight: 800
                            .badges_earned
                                .summary
                                    .summary_content
                                        padding: 20px 30px
                        .top_booked
                            max-width: calc(100% - 60px)
                            .tab_content_main
                                .instructor
                                    .item
                                        .num_wrapper
                                            width: 40px
                                            height: 40px
                                            .num
                                                neutra_bold(20px, false)
                                                font-weight: 800
                                        .overlay
                                            .info
                                                .num_wrapper_alt
                                                    width: 35px
                                                    height: 35px
                                                    .num
                                                        neutra_bold(20px, false)
                                                        font-weight: 800
                                                .name
                                                    neutra_bold(18px, false)
                                            .rides
                                                neutra_book(14px, false)
                        .chart
                            max-width: calc(100% - 60px)
                            .ride_summary
                                .left,
                                .right
                                    .value
                                        neutra_bold(35px, true)
                                        font-weight: 800
    #default_menu
        .menu_tab
            .menu_tab_item
                neutra_bold(14px, true)
                margin-right: 20px
    .default_table
        tbody
            tr
                td
                    .package_info
                        .title
                            neutra_bold(18px, false)
    .update_profile
        #content
            .side_menu_tab
                .left
                    padding: 40px 0 40px 40px
                    h1
                        neutra_bold(20px, false)
                    .side_menu
                        .side_menu_item
                            .item
                                neutra_book(16px, false)
                .right
                    .update_profile_tab_content
                        .wrapper
                            padding: 40px
    .deactivate_account
        #content
            max-width: 85%
    /**
     * Referral */
    #referral
        .wrapper
            h2
                neutra_bold(22px, false)
            h3
                neutra_book(14px, false)
                margin: 20px 0 40px
    /**
     * Promos */
    #promos
        &.alt
            .promo_list
                .title
                    neutra_bold(24px, false)
                .copy_wrapper
                    .code
                        neutra_bold(24px, true)
        .promo_slide
            .overlay
                .title
                    neutra_bold(20px, false)
                .description
                    neutra_book(14px, false)
    /**
     * Packages */
    #packages
        &.alt
            .header
                padding: 50px 0 30px
                h2
                    neutra_bold(24px, false)
                    text-transform: none
                .description
                    p
                        neutra_book(16px, false)
        .header
            h2
                neutra_bold(24px, true)
            .description
                p
                    neutra_book(16px, false)
                    margin-bottom: 20px
                .link
                    neutra_bold(16px, false)
        .content
            .package_wrapper
                &.promo,
                &.alt,
                &.alt_2
                    .ribbon
                        neutra_bold(12px, true)
                .package_header
                    .title
                        neutra_bold(20px, false)
                    .description
                        neutra_book(12px, true)
                        font-weight: 500
                        margin: 30px 0 60px
