@import 'fonts'

@media (max-width: 1440px) and (min-width: 1281px)

    .default_modal
        .confirmation_wrapper
            width: 400px
            padding: 40px 40px 30px
            .confirmation_text
                brandon_regular(16px, false)
            .confirmation_title
                brandon_bold(18px, false)
                padding: 0 0 10px
            .button_group
                margin-top: 30px
                &.alt_2
                    margin-top: 15px


    .default_btn,
    .default_btn_out,
    .default_btn_wht,
    .default_btn_blk,
    .default_btn_blue
        brandon_bold(12px, true)
        padding: 10px 30px

    .default_btn_img
        brandon_bold(12px, true)
        .btn_wrapper
            span
                padding: 10px 30px
                &.img
                    padding: 10px
                    img
                        width: 15px

    .back
        brandon_medium(12px, true)

    .form_group
        margin-bottom: 20px
        label
            brandon_medium(12px, true)
        .validation_errors
            brandon_medium(12px, false)

    .form_flex
        &.with_btn
            .form_button
                .default_btn_out
                    padding: 10px 30px
        &.radio
            > label
                brandon_medium(12px, true)
            .validation_errors
                brandon_medium(12px, false)

    .form_radio
        label
            brandon_medium(12px, true)
        .validation_errors
            brandon_medium(12px, false)

    .form_check
        label
            brandon_medium(12px, true)
        .validation_errors
            brandon_medium(12px, false)

    .input_text,
    .input_select
        brandon_regular(14px, false)
        padding: 8px 10px

    .input_link,
    .form_disclaimer
        brandon_regular(14px, false)

    .login_sign_up
        .close_icon
            top: calc(0% + 15px)
        &.front,
        &.not_front
            margin-top: 60px
            &.sticky
                margin-top: 60px
        #login
            height: calc(100% - 150px)
            overflow-y: scroll
            overflow-x: hidden
            padding: 0 30px
            max-width: 40%
            #default_form,
            .new_here
                margin-top: 30px
            .new_here
                brandon_medium(14px, false)
            .action
                .default_btn_login
                    margin: 30px 0
                    span
                        brandon_medium(14px, false)
            .title
                brandon_bold(18px, false)

    /**
     * Main */
    #main_container
        #header
            &.instructor,
            &.not_front,
            &.front
                .navbar
                    padding: 10px
            .navbar
                flex: 1 0 70%
                .logo
                    img
                        width: 30px
                    .logo_title
                        neutra_bold(16px, true)
                        padding-left: 10px
                .nav_list
                    li
                        margin: 0 20px
                        .nav_item
                            brandon_bold(12px, true)
                .user_dropdown
                    .user_dropdown_list
                        width: 220px
                        .user_dropdown_item
                            .item_link
                                brandon_regular(14px, false)
                                padding: 10px 15px
                    h3
                        brandon_bold(14px, false)
                        margin-left: 10px
            .nav_burger
                flex: 0 0 35px
                margin: 0 10px
                .bar
                    &.bottom_line
                        width: calc(100% - 15px)
        #navbar_expanded
            min-height: 200px
            overflow-y: scroll
            overflow-x: hidden
            &.sticky
                .logo
                    padding: 10px 15px
                .close_icon
                    top: calc(0% + 15px)
                    right: 0
            .close_icon
                top: calc(0% + 15px)
                right: 0
            .logo
                padding: 10px 15px
                img
                    width: 30px
                .logo_title
                    neutra_bold(16px, true)
                    padding-left: 10px
                    letter-spacing: 3px
            .content
                padding: 20px 15px
                .left
                    margin-bottom: 30px
                    .expanded_main_list
                        li
                            margin-bottom: 40px
                            &:last-child
                                margin-bottom: 0
                            .main_item
                                &.nuxt-link-exact-active,
                                &.nuxt-link-active,
                                &:hover,
                                &:focus
                                    &::before
                                        right: calc(0% - 35px)
                                #menu
                                    width: auto
                                    height: 40px
                .right
                    .expanded_nav_list
                        li
                            margin-bottom: 20px
                            .nav_item
                                brandon_bold(14px, true)
                    .expanded_nav_description
                        padding-top: 10px
                        .link
                            margin-bottom: 20px
                            span
                                brandon_bold(14px, false)
                        .social_links
                            margin-top: 20px
                            brandon_regular(14px, false)
                            a
                                margin: 0 10px
                .left,
                .right
                    flex: 0 0 45%
        #footer
            padding: 40px
            .footer_link
                &.alt
                    p
                        brandon_regular(14px, false)
                        a
                            brandon_bold(14px, false)
                h3
                    brandon_medium(14px, false)
                ul
                    li
                        .footer_item
                            brandon_regular(14px, false)
                .social_links
                    h4
                        brandon_medium(14px, false)
            .back_to_top
                right: calc(0% - 40px)
        #pro_tip
            top: calc(0% + 61px)
            padding: 5px 15px
            .tip
                brandon_regular_italic(14px, false)

    /**
     * Home */
    .home
        #reviews
            .header
                margin: 0
                .description
                    p
                        strong
                            brandon_bold(22px, true)
            .content
                .review_slide
                    img
                        height: 50px
                    .description
                        brandon_regular(14px, false)
        #instructors
            padding: 25em 0
            .overlay
                .info
                    &::before
                        background-size: contain
                    .title
                        brandon_bold(22px, false)
                    .description
                        brandon_regular(16px, false)
                        margin: 30px 0
        #studios
            .header
                padding: 30px 0
                .description
                    margin-right: 40px
                    p
                        strong
                            brandon_bold(22px, true)
            .content
                .studio_content
                    max-width: calc(100% - 75px)
                    .wrapper
                        padding-bottom: 60px
                        .right
                            .overlay
                                padding: 30px
                                .content_select
                                    .input
                                        .select
                                            select
                                                width: auto
                                .content_flex
                                    .title
                                        brandon_medium(30px, false)
                                        margin-top: 15px
                                    .description
                                        margin: 20px 0
                                        label
                                            brandon_regular(16px, true)
                                        .opening
                                            p
                                                brandon_regular(14px, false)
                                    .default_btn
                                        margin-top: 20px
        #lets_ride
            .header
                padding: 30px 0
                .description
                    p
                        span
                            brandon_bold(24px, false)

    /**
     * Buy Rides */
    .buy_rides
        &.inner
            #payments
                max-width: calc(85%)
                &.dismissed,
                &.dismiss
                    padding: 40px 0 60px
                .step
                    .header_title
                        brandon_bold(20px, false)
                        margin-bottom: 30px
                    .wrapper
                        .left
                            padding-right: 40px
                            .header
                                h2
                                    brandon_regular(16px, false)
                            .content
                                ul
                                    li
                                        brandon_regular(12px, false)
                        .right
                            padding-left: 40px
                            .breakdown_list
                                .item
                                    p
                                        brandon_regular(14px, false)
                                .total
                                    margin: 30px 0 0
                                    p
                                        brandon_bold(16px, false)
                            .breakdown_actions
                                margin: 30px 0 20px
                    .preview_payment
                        .preview
                            .available
                                .available_item
                                    h3
                                        brandon_regular(16px, false)
                                    p
                                        brandon_medium(16px, false)
                                    .unavailable
                                        a
                                            brandon_medium(12px, false)

                            .item
                                h3,
                                p
                                    brandon_regular(16px, false)
                            .total
                                margin: 30px 0
                                p
                                    brandon_bold(16px, false)
        #digital
            .overlay
                h2
                    brandon_bold(30px, false)
                h3
                    brandon_regular(16px, false)

    /**
     * Book a bike */
    .book_a_bike
        &.landing
            #content
                .left
                    padding: 30px 30px 20em
                    .filter
                        .wrapper
                            &.studio_filter
                                .group
                                    margin-bottom: 10px
                                    label
                                        brandon_medium(14px, false)
                            &.instructor_filter
                                margin-top: 40px
                                .autocomplete
                                    &::before
                                        top: calc(0% + (42px / 2))
                                    .text
                                        brandon_regular(14px, false)
                                        padding: 10px 35px
                                    .autocomplete_dropdown
                                        .list
                                            img
                                                width: 30px
                                                height: 30px
                                            p
                                                brandon_regular(16px, false)
                            h3
                                margin-bottom: 15px
                                brandon_regular(14px, true)
                .right
                    padding: 30px
                    .date_navigator
                        .date
                            .overlay
                                .abbr
                                    brandon_bold(18px, false)
                                    margin-bottom: 15px
                                .month
                                    brandon_black(12px, true)
                                .day
                                    brandon_black(40px, true)
                    .schedule_list
                        .header
                            span
                                brandon_regular(18px, false)
                                &.label
                                    brandon_regular(18px, false)
                        .content
                            .schedule
                                padding: 20px 40px
                                .time
                                    brandon_bold(16px, true)
                                .class
                                    .image
                                        width: 50px
                                        height: 50px
                                    .info
                                        padding-left: 20px
                                        h2
                                            brandon_medium(14px, false)
                                        h3
                                            brandon_regular(14px, false)
                                        .ride
                                            brandon_medium(18px, false)
                        .no_schedule
                            p
                                brandon_black(35px, true)

    /**
     * Instructors */
    .instructors
        margin-top: 60px
        #breadcrumb
            &.has_padding
                padding-top: 45px
        #instructors_nav
            top: calc(0% + 60px)
            .left
                .select
                    .label
                        brandon_bold(16px, false)
                    .custom_select
                        .check
                            label
                                brandon_medium(14px, false)
                                padding: 15px 20px
                                span
                                    padding-left: 15px
                                &.label
                                    padding: 15px 20px
                                    img
                                        height: 25px
            .right
                .input
                    input
                        width: 200px
                        brandon_regular(16px, false)
            .left,
            .right
                padding: 10px
        #instructors_content
            .header
                padding: 40px 0 30px
                h1
                    brandon_bold(40px, true)
                .subheader
                    h2
                        brandon_regular(16px, false)
                    .select
                        select
                            brandon_regular(16px, false)
            .content
                padding: 0 20px
                margin-bottom: 30px
                .wrapper
                    .overlay
                        .title
                            brandon_bold(14px, true)
                            padding: 0 10px
                        .action
                            padding: 20px 10px 10px
                .load
                    margin-top: 30px

    /**
     * Studios */
    .studios
        #teaser
            padding: 30px 0
            p
                brandon_regular(14px, false)
        #visit
            padding: 30px 0
            h2
                brandon_bold(30px, true)
            .content
                .wrapper
                    h2
                        brandon_bold(16px, true)
        #studio_ig
            .header
                .description
                    span
                        brandon_bold(24px, false)

    #referral
        .wrapper
            h2
                brandon_bold(22px, false)
            h3
                brandon_regular(14px, false)
                margin: 20px 0 40px

    #promos
        .promo_slide
            .overlay
                .title
                    brandon_bold(20px, false)
                .description
                    brandon_regular(14px, false)

    #packages
        .header
            h2
                brandon_bold(24px, true)
            .description
                p
                    brandon_regular(16px, false)
                    margin-bottom: 20px
                .link
                    brandon_bold(16px, false)
        .content
            max-width: calc(100% - 60px)
            .package_wrapper
                &.promo
                    .ribbon
                        brandon_bold(12px, true)
                .package_header
                    .title
                        brandon_bold(20px, false)
                    .description
                        brandon_medium(12px, true)
                        margin: 30px 0 60px
                .discounted_price
                    brandon_regular(12px, false)
                .price
                    brandon_bold(14px, true)

    #breadcrumb
        padding-top: 60px
        &.dismiss
            padding-top: 90px
        ul
            li
                .front,
                .slash
                    brandon_regular(12px, false)

    #banner
        &.mt
            margin-top: 60px
        &.alt_2
            .overlay_mid
                h1
                    brandon_bold(26px, false)
        .overlay_mid
            h1
                brandon_bold(30px, true)
            h2
                brandon_bold(16px, false)
                margin-top: 10px
        .overlay_top
            .overlay_image
                width: 600px
        .overlay_bottom
            p
                brandon_regular(16px, false)

@media (max-width: 1280px) and (min-width: 768px)

    .default_btn,
    .default_btn_out,
    .default_btn_wht
        brandon_bold(12px, true)
        padding: 10px 20px

    .default_modal
        .confirmation_wrapper
            width: 400px
            padding: 40px 20px 30px
            .confirmation_highlight
                brandon_bold(18px, false)
                margin-top: 20px
            .confirmation_title
                brandon_bold(18px, false)
                padding: 0
            .confirmation_text
                brandon_regular(16px, false)
            .default_btn,
            .default_btn_out
                width: auto
                brandon_bold(14px, true)

    /**
     * Main */
    #main_container
        #header
            &.not_front,
            &.front
                .navbar
                    padding: 10px
            .navbar
                flex: 1 0 70%
                .logo
                    img
                        width: 40px
                    .logo_title
                        neutra_bold(16px, true)
                        padding-left: 10px
            .nav_burger
                flex: 0 0 40px
                margin: 0 10px
                .bar
                    &.bottom_line
                        width: calc(100% - 15px)
        #navbar_expanded
            &.sticky
                .logo
                    padding: 10px 15px
                .close_icon
                    top: calc(0% + 10px)
                    right: calc(0% + 15px)
            .close_icon
                top: calc(0% + 10px)
                right: calc(0% + 15px)
            .logo
                padding: 10px 15px
                img
                    width: 40px
                .logo_title
                    neutra_bold(16px, true)
                    padding-left: 10px
                    letter-spacing: 3px
            .content
                padding: 20px 15px
                .left
                    margin-bottom: 30px
                    .expanded_main_list
                        li
                            margin-bottom: 40px
                            &:last-child
                                margin-bottom: 0
                            .main_item
                                &.nuxt-link-exact-active,
                                &.nuxt-link-active,
                                &:hover,
                                &:focus
                                    &::before
                                        right: calc(0% - 35px)
                                #menu
                                    width: auto
                                    height: 30px
                .right
                    .expanded_nav_list
                        li
                            margin-bottom: 20px
                            .nav_item
                                brandon_bold(16px, true)
                    .expanded_nav_description
                        padding-top: 10px
                        .link
                            margin-bottom: 20px
                            p
                                brandon_regular(12px, false)
                            span
                                brandon_bold(12px, false)
                        p
                            brandon_regular(12px, false)
                            a
                                brandon_bold(12px, false)
                        .social_links
                            margin-top: 20px
                            h4
                                brandon_regular(12px, false)
                            a
                                margin: 0 10px
                .left,
                .right
                    flex: 0 0 48%
        #footer
            padding: 20px
            .logo
                flex: 0 0 100%
                img
                    width: 50px
            .footer_link
                flex: 0 0 50%
                height: 0px
                overflow-y: hidden
                transition: .3s ease-in-out
                &.active
                    margin-bottom: 10px
                &.alt
                    flex: 0 0 100%
                    height: 100%
                    .social_links
                        margin-bottom: 20px
            .back_to_top
                top: calc(0% + 50px)
                right: calc(0% + 30px)
                transform: translateY(0%) rotate(90deg)
        #pro_tip
            top: calc(0% + 61px)
            padding: 5px 15px
            .tip
                brandon_regular_italic(14px, false)

    #promos
        .promo_slide
            img
                width: 100%
                height: 450px
                object-fit: cover
                object-position: center
            .overlay
                max-width: 400px
                left: calc(0% + 5%)
                .title
                    brandon_bold(20px, false)
                    line-height: 1.25
                .description
                    brandon_regular(14px, false)
                .copy_wrapper
                    margin-top: 20px
                    .code
                        brandon_bold(20px, false)
                    .default_btn
                        padding: 10px
        .action_mobile
            text-align: center
            .default_btn
                width: auto
                margin-top: 20px

    #packages
        .header
            padding: 10px 30px 30px
            .description
                margin-right: 0
                p
                    brandon_regular(16px, false)
                    margin-bottom: 0
        .content
            padding-bottom: 20px
            max-width: calc(100% - 60px)
            .swiper-container
                padding-bottom: 40px
                margin-bottom: 20px
                .package_wrapper
                    height: inherit
                    margin: 0
                    .package_header
                        .title
                            brandon_bold(24px, false)
                        .description
                            brandon_regular(14px, true)
                            margin: 30px 0
                    .default_btn_out
                        width: auto
        .default
            .swiper-pagination
                bottom: calc(0%) !important
                .swiper-pagination-bullet
                    background: rgba(38, 164, 139, 0.4)
                    &.swiper-pagination-bullet-active
                        background: rgba(38, 164, 139, 1)
    .home
        #reviews
            .header
                margin-top: 10px
                padding: 0 30px
                .description
                    flex: 0 0 45%
                    margin-right: 0
                    p
                        brandon_regular(16px, true)
                        margin-bottom: 0
                        strong
                            brandon_bold(24px, true)
            .content
                padding: 30px
                .swiper-container
                    padding-bottom: 60px
                    .review_slide
                        .description
                            brandon_regular(16px, false)
            .default
                .swiper-pagination
                    bottom: calc(0%) !important
                    .swiper-pagination-bullet
                        background: rgba(38, 164, 139, 0.4)
                        &.swiper-pagination-bullet-active
                            background: rgba(38, 164, 139, 1)
        #instructors
            padding: 30em 0
            .overlay
                top: 50%
                padding: 0 15px
                .image
                    text-align: center
                    img
                        width: 280px
                        {image_default}
                .info
                    padding-left: 0
                .image,
                .info
                    flex: 0 0 100%
        #studios
            .header
                padding: 30px
                .description
                    flex: 0 0 45%
                    margin-right: 0
                    p
                        brandon_regular(16px, true)
                        margin-bottom: 0
                        strong
                            brandon_bold(24px, true)
            .content
                .studio_content
                    max-width: calc(100% - 60px)
                    .wrapper
                        padding-bottom: 20px
                        .right
                            background-color: transparent
                            .overlay
                                position: static
                                padding: 30px 15px
                                transform: translateY(0%)
                                .content_select
                                    .input
                                        display: flex
                                        flex-flow: row wrap
                                        align-items: center
                                        width: 100%
                                        label
                                            flex: 0 0 auto
                                            brandon_regular(18px, false)
                                        select
                                            flex: 0 0 auto
                                            width: auto
                                            brandon_bold(18px, false)
                        .left
                            display: flex
                            flex-flow: row wrap
                            align-items: flex-start
                            justify-content: space-between
                            .content_flex
                                flex: 0 0 calc(50% - 30px)
                                .default_btn
                                    width: auto
                            .studio_slide
                                .overlay
                                    bottom: calc(0% + 35px)
                                    .title
                                        brandon_regular(14px, true)
                        .left,
                        .right
                            flex: 0 0 100%
                            max-width: 100%
                            .content_flex
                                margin: 15px
                                .title
                                    brandon_medium(22px, false)
                                    margin-top: 0
                                .description
                                    margin: 0 0 20px
                                    label
                                        brandon_regular(12px, true)
                                        margin-bottom: 10px
                                    p
                                        brandon_regular(16px, false)
                                        margin-bottom: 5px
                                    .link
                                        a
                                            brandon_regular(16px, false)
                                .default_btn
                                    margin-top: 10px
        #lets_ride
            .header
                padding: 30px
                .description
                    flex: 0 0 45%
                    margin-right: 0
                    p
                        brandon_regular(16px, false)
                        margin-bottom: 0
                        span
                            brandon_bold(24px, false)

    .login_sign_up
        &.front,
        &.not_front
            margin-top: 74px
            &.sticky
                margin-top: 74px
        #login
            max-width: 65%
            .subtitle
                brandon_regular(14px, false)
                margin: 10px 0 20px
            .sign_up_header
                .counter
                    brandon_bold(12px, false)
            .new_here
                margin-top: 20px
                brandon_medium(14px, false)
            .divider
                span
                    brandon_regular(16px, false)
            #default_form
                margin-top: 25px
            .title
                brandon_bold(18px, false)
            .action
                .default_btn_login
                    margin: 25px 0
                    span
                        brandon_bold(14px, false)

    .back
        brandon_bold(12px, true)

    .form_disclaimer
        brandon_regular(12px, false)

    .form_group
        margin-bottom: 20px
        &.disclaimer
            margin-bottom: 10px
        label
            brandon_medium(12px, true)
        .validation_errors
            brandon_medium(12px, false)

    .form_radio
        label
            brandon_medium(12px, true)
            margin-left: 10px
        .validation_errors
            brandon_medium(12px, false)

    .form_flex
        &.radio
            & > label
                brandon_medium(12px, true)
            .validation_errors
                brandon_medium(12px, false)

    .form_check
        .input_check
            width: 18px
            height: 18px
            padding-left: 18px
        label
            brandon_medium(12px, true)

    .input_link
        brandon_regular(14px, false)

    .input_text,
    .input_select
        brandon_regular(14px, false)
        padding: 10px

    #banner
        &.mt
            margin-top: 74px
        &.alt_2
            .overlay_mid
                h1
                    brandon_bold(26px, false)
        img
            width: 100%
        .main_image
            height: 800px
        .overlay_top
            top: 74px
            width: 100%
            transform: translateY(0%)
            .overlay_image
                width: 100%
                height: auto
                object-fit: contain
                animation: slide_from_top 1.5s
            .overlay_child
                margin-top: 50px
                display: flex
                flex-flow: row wrap
                align-items: center
                justify-content: center
                position: static
                transform: translateY(-50%) translateX(0)
                animation: slide_from_top 1.5s
                #play_icon
                    width: 35px
                    height: 35px
                .label
                    brandon_medium(18px, true)
                    letter-spacing: 1.6px
                    margin-top: 0
                    padding-left: 10px
        .overlay_mid
            h1
                brandon_bold(30px, true)
            h2
                brandon_bold(18px, false)
        .overlay_bottom
            justify-content: center
            max-width: calc(100% - 30px)
            bottom: calc(0% + 30px)
            animation: slide_from_bottom 1.5s
            p
                flex: 0 0 100%
