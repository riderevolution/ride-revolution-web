            .right
                flex: 0 0 45%
                animation: slide_from_right 1.5s
                .expanded_nav_list
                    list-style: none
                    margin: 0
                    padding: 0
                    li
                        margin-bottom: 40px
                        .nav_item
                            position: relative
                            brandon_bold(18px, true)
                            color: $black
                            transition: .3s ease-in-out
                            &::before
                                content: ''
                                position: absolute
                                top: 100%
                                left: 0
                                width: 0
                                height: 3px
                                z-index: 1
                                background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
                                opacity: 0
                                transition: .3s ease-in-out .3s
                            &::after
                                content: ''
                                position: absolute
                                top: 100%
                                left: calc(100% - 25%)
                                width: 0
                                height: 3px
                                z-index: 1
                                background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
                                opacity: 0
                                transition: .3s ease-in-out .05s
                            &:hover
                                color: $green
                                text-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
                                transition: .3s ease-in-out
                                &::before
                                    width: calc(100% - 30%)
                                    opacity: 1
                                    transition: .3s ease-in-out .05s
                                &::after
                                    width: calc(0% + 25%)
                                    opacity: 1
                                    transition: .3s ease-in-out .3s
                .expanded_nav_description
                    padding-top: 40px
                    .link
                        display: flex
                        flex-flow: row wrap
                        align-items: center
                        margin-bottom: 30px
                        p
                            brandon_regular(16px, false)
                            color: $black
                            margin-right: 10px
                        span
                            brandon_bold(16px, false)
                            padding-left: 5px
