@import 'fonts'

$green = #26A48B
$green_alt = #78F7DE
$green_alt2 = #82BCB1
$green_alt3 = #C4FFF3

$dark_green = #0E3439

$blue = #51A6EA
$blue_alt = #7AD1F7
$blue_alt2 = #E3F7FF
$blue_alt3 = #3BB7F2
$blue_alt4 = #199DDB

$pink = #9E558B
$pink_alt = #FD649C

$red = #F13747
$red_alt = #FFE5E6

$white = #FFF
$white_alt = #F7F7F7

$black = #171717
$black_alt = #4E4E4E
$black_alt2 = #53565C

$gray = #A8A8A8
$gray_alt = #ABADB9
$gray_alt2 = #808080
$gray_alt3 = #F7F7F7

$border = #DEDEDE
$border_alt = #EDEEF0
$border_alt2 = #9C9EAA
$border_alt3 = #A0A0A0
$border_alt4 = #CECECE

image_default =
    height: auto
    vertical-align: middle
    object-fit: cover
    -webkit-user-drag: none

html
    -webkit-text-size-adjust: none
    -webkit-font-smoothing: subpixel-antialiased
    backface-visibility: hidden

*,
*:before,
*:after
    user-select: none
    box-sizing: border-box
    margin: 0
    word-break: break-word !important

body
    position: relative
    overflow-x: hidden
    &.no_click
        pointer-events: none
    &.no_scroll
        overflow-y: hidden

::-webkit-scrollbar
    width: 20px
    background-color: $white
::-webkit-scrollbar-track
    box-shadow: none
::-webkit-scrollbar-thumb
    background-color: $green
    border: 6px solid rgba(0, 0, 0, 0)
    background-clip: padding-box
    border-radius: 50px

input,
input:before,
input:after,
textarea,
textarea:before,
textarea:after,
select,
select:before,
select:after
    user-select: initial

h1,
h2,
h3,
h4,
h5
    font-weight: normal

table
    width: 100%
    border-collapse: collapse

img
    width: auto
    max-width: 100%
    {image_default}
    transition: .3s ease-in-out

p
    .green
        color: $green

a
    text-decoration: none

svg
    font-family: Brandon-Bold

.no_font
    font-size: 0

.invalid_token,
.reset_done
    text-align: center
    height: 20vh
    vertical-align: middle
    display: flex
    justify-content: center
    align-items: center
    brandon_medium(30px, false)
    color: $black
    padding: 0 20px
    .link
        color: $green
        padding: 0 5px
        cursor: pointer

.no_results
    text-align: center
    margin: 30px 0
    .logo
        filter: grayscale(1)
        opacity: 0.3
    .text
        brandon_regular(28px, false)
        margin-bottom: 30px
    .default_btn
        margin-top: 30px
    &.alt
        text-align: center
        width: 100%
        max-width: 75%
        margin: 40px auto
        p
            brandon_black(50px, true)
            color: #D0D0D0

.error_page
    &.alternate
        height: 50vh
        .container
            .header_title
                brandon_bold(40px, false)
    height: 100vh
    position: relative
    animation: slide_from_top 1s
    .container
        position: absolute
        top: 50%
        left: 0
        right: 0
        text-align: center
        transform: translateY(-50%)
        .logo
            filter: grayscale(1)
            opacity: 0.3
        .header_title
            brandon_bold(40px, false)
            margin: 20px 0
        .header_subtitle
            neutra_book(36px, false)
        .default_btn
            margin-top: 40px

/**
 * Pagination */
#cms_pagination
    display: flex
    flex-flow: row wrap
    align-items: center
    justify-content: center
    padding: 20px 0 30px
    user-select: none
    .separator
        brandon_bold(16px, false)
        color: $green
        padding: 0 5px
    .next,
    .prev
        brandon_regular(16px, false)
        color: $white
        background-color: $green
        border: 1px solid $green
        padding: 8px 14px
        margin: 0 3px
        cursor: pointer
        transition: .3s ease-in-out
        &:hover,
        &:focus
            background-color: $green_alt3
            color: $green
            transition: .3s ease-in-out
        &.disabled
            background-color: #e2e2e2
            color: $black
            border: 1px solid #e2e2e2
            transition: .3s ease-in-out
    .number
        brandon_regular(16px, false)
        color: $green
        background-color: $green_alt3
        border: 1px solid $green
        padding: 8px 14px
        margin: 0 3px
        cursor: pointer
        transition: .3s ease-in-out
        &:hover,
        &:focus,
        &.active
            background-color: $green
            color: $white
            transition: .3s ease-in-out
        &.active
            pointer-events: none

/**
 * Default Buttons */
.back
    brandon_medium(16px, true)
    color: $black
    padding: 10px 10px 10px 30px
    border: 1px solid transparent
    background-image: url('/icons/back-arrow-icon.svg')
    background-color: transparent
    background-position: calc(0% + 10px) center
    background-repeat: no-repeat
    cursor: pointer
    transition: .3s ease-in-out
    &:hover
        border: 1px solid $black
        transition: .3s ease-in-out

.default_btn
    display: inline-block
    text-align: center
    width: auto
    brandon_bold(16px, true)
    color: $white
    letter-spacing: .7px
    padding: 15px 45px
    background-color: transparent
    border: none
    outline: none
    cursor: pointer
    background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
    box-shadow: 0px 0px 30px transparent
    transition: .3s ease-in-out
    &.ml
        margin-left: 15px
    &.disabled
        pointer-events: none
        background-image: linear-gradient(25deg, #E9E9E9 0%, #E9E9E9 100%)
        border: 1px solid #E9E9E9
        color: $gray_alt2
    &.full
        width: 100%
    &:hover
        box-shadow: 0px 0px 20px rgba(38, 164, 139, 0.75)
        transition: .3s ease-in-out

.default_btn_wht
    display: inline-block
    text-align: center
    width: auto
    brandon_bold(16px, true)
    color: $green
    letter-spacing: .7px
    padding: 15px 45px
    background-color: $white
    border: none
    outline: none
    box-shadow: 0px 0px 30px transparent
    cursor: pointer
    transition: .3s ease-in-out
    &.ml
        margin-left: 15px
    &.mb
        margin-bottom: 15px
    &:hover
        box-shadow: 0px 0px 20px rgba(255, 255, 255, 0.75)
        transition: .3s ease-in-out

.default_btn_blue
    display: inline-block
    text-align: center
    width: auto
    brandon_bold(16px, true)
    color: $white
    letter-spacing: .7px
    padding: 15px 45px
    background-color: $blue_alt3
    border: none
    outline: none
    box-shadow: 0px 0px 30px transparent
    cursor: pointer
    transition: .3s ease-in-out
    &.disabled
        cursor: not-allowed
        pointer-events: none
        background-color: $gray_alt3
        color: $gray
        border: 1px solid $gray_alt3
        transition: .3s ease-in-out
    &.mb
        margin-bottom: 15px
    &:hover
        box-shadow: 0px 0px 20px rgba(59, 183, 242, 0.75)
        transition: .3s ease-in-out

.default_btn_wht_alt
    cursor: pointer
    box-shadow: 0px 0px 30px transparent
    transition: .3s ease-in-out
    &.green
        .text
            display: block
            color: $green
            border: 1px solid $green_alt2
            .border_top
                position: absolute
                top: calc(0% - 2px)
                left: calc(0% - 2px)
                opacity: 1
                transition: .5s ease-in-out
                &.left
                    width: 20px
                    border-top: 3px solid $green
                    &.alt
                        opacity: 1
                        width: 15px
                        left: calc(5% + 10px)
                &.right
                    height: 20px
                    border-left: 3px solid $green
            .border_bottom
                position: absolute
                bottom: calc(0% - 2px)
                right: calc(0% - 2px)
                opacity: 1
                transition: .5s ease-in-out
                &.left
                    width: 20px
                    border-bottom: 3px solid $green
                &.right
                    height: 20px
                    border-right: 3px solid $green
        &:hover
            .text
                color: $white
                border: none
                transition: .3s ease-in-out
                .border_top,
                .border_bottom
                    opacity: 0
                    transition: .5s ease-in-out
                    &.left
                        width: calc(100% + 4px)
                        &.alt
                            opacity: 0
                    &.right
                        height: calc(100% + 4px)
                &::before
                    opacity: 1
                    transition: .3s ease-in-out
    .text
        position: relative
        display: inline-block
        text-align: center
        width: auto
        brandon_bold(16px, true)
        color: $white
        letter-spacing: 1.3px
        padding: 15px 45px
        background-color: transparent
        border: 1px solid $white
        outline: none
        transition: .3s ease-in-out
        span
            position: inherit
        &::before
            content: ''
            position: absolute
            z-index: 0
            top: 0
            left: 0
            right: 0
            height: 100%
            opacity: 0
            background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
            box-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
            transition: .3s ease-in-out
        .border_top
            position: absolute
            top: calc(0% - 2px)
            left: calc(0% - 2px)
            transition: .5s ease-in-out
            &.left
                width: 20px
                border-top: 3px solid $white
                &.alt
                    opacity: 1
                    width: 15px
                    left: calc(5% + 10px)
            &.right
                height: 20px
                border-left: 3px solid $white
        .border_bottom
            position: absolute
            bottom: calc(0% - 2px)
            right: calc(0% - 2px)
            transition: .5s ease-in-out
            &.left
                width: 20px
                border-bottom: 3px solid $white
            &.right
                height: 20px
                border-right: 3px solid $white
    &:hover
        box-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
        transition: .5s ease-in-out
        .text
            border: 1px solid $green
            background-color: rgba(255, 255, 255, 0.1)
            transition: .5s ease-in-out
            .border_top,
            .border_bottom
                transition: .5s ease-in-out
                &.left
                    width: calc(100% + 4px)
                    &.alt
                        opacity: 0
                &.right
                    height: calc(100% + 4px)
            .border_top
                &.left
                    border-top: 3px solid $green
                &.right
                    border-left: 3px solid $green
            .border_bottom
                &.left
                    border-bottom: 3px solid $green
                &.right
                    border-right: 3px solid $green

.default_btn_blk
    display: inline-block
    text-align: center
    width: auto
    brandon_bold(16px, true)
    color: $black
    letter-spacing: .7px
    padding: 10px 45px
    background-color: $white
    outline: none
    box-shadow: 0px 0px 30px transparent
    border: 1px solid $black
    cursor: pointer
    transition: .3s ease-in-out
    &.mb
        margin-bottom: 15px
    &:hover
        color: $white
        background-color: $black
        box-shadow: 0px 0px 30px rgba(0, 0, 0, 0.75)
        transition: .3s ease-in-out

.default_btn_red
    display: inline-block
    text-align: center
    width: auto
    brandon_bold(16px, true)
    color: $red
    letter-spacing: .7px
    padding: 14px 45px
    background-color: $white
    outline: none
    box-shadow: 0px 0px 30px transparent
    border: 1px solid $red
    cursor: pointer
    transition: .3s ease-in-out
    &.mb
        margin-bottom: 15px
    &:hover
        color: $white
        background-color: $red
        box-shadow: 0px 0px 30px rgba(241, 55, 71, 0.75)
        transition: .3s ease-in-out

.default_btn_blk_alt
    brandon_bold(14px, true)
    line-height: 1
    letter-spacing: .7px
    color: $black
    display: inline-block
    margin: 0 auto
    cursor: pointer
    transition: .3s ease-in-out
    span
        padding-left: 10px
        vertical-align: middle
    &:hover
        color: $gray
        transition: .3s ease-in-out

.default_btn_out
    display: inline-block
    text-align: center
    position: relative
    brandon_bold(16px, true)
    color: $green
    letter-spacing: .7px
    outline: none
    padding: 15px 45px
    background-color: transparent
    border: 1px solid $green_alt2
    cursor: pointer
    transition: .3s ease-in-out
    &.disabled
        pointer-events: none
        border: 1px solid $gray
        color: $gray
    span
        position: inherit
    &::before
        content: ''
        position: absolute
        z-index: 0
        top: 0
        left: 0
        right: 0
        height: 100%
        opacity: 0
        background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
        box-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
        transition: .3s ease-in-out
    &:hover
        color: $white
        border: 1px solid transparent
        transition: .3s ease-in-out
        &::before
            opacity: 1
            transition: .3s ease-in-out

.default_btn_wht_out
    display: inline-block
    text-align: center
    position: relative
    brandon_bold(16px, true)
    color: $white
    letter-spacing: .7px
    outline: none
    padding: 15px 45px
    background-color: transparent
    border: 1px solid $white
    cursor: pointer
    transition: .3s ease-in-out
    &.disabled
        pointer-events: none
        border: 1px solid $gray
        color: $gray
    span
        position: inherit
    &::before
        content: ''
        position: absolute
        z-index: 0
        top: 0
        left: 0
        right: 0
        height: 100%
        opacity: 0
        background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
        box-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
        transition: .3s ease-in-out
    &:hover
        color: $white
        border: 1px solid $green_alt3
        transition: .3s ease-in-out
        &::before
            opacity: 1
            transition: .3s ease-in-out

.default_btn_img
    display: inline-block
    text-align: center
    position: relative
    brandon_bold(16px, true)
    color: $white
    letter-spacing: .7px
    outline: none
    background-color: $blue_alt3
    border: 1px solid $blue_alt4
    cursor: pointer
    transition: .3s ease-in-out
    &.disabled
        cursor: not-allowed
        pointer-events: none
        background-color: $gray_alt3
        color: $gray
        border: 1px solid $gray_alt3
        transition: .3s ease-in-out
        .btn_wrapper
            span
                &.img
                    border-right: 1px solid $gray
                    filter: grayscale(1)
    .btn_wrapper
        position: inherit
        display: flex
        flex-flow: row wrap
        align-items: center
        justify-content: space-between
        span
            padding: 13px 30px
            &.img
                padding: 13px 10px
                border-right: 1px solid $blue_alt4
    span
        position: inherit
    &::before
        content: ''
        position: absolute
        z-index: 0
        top: 0
        left: 0
        right: 0
        height: 100%
        opacity: 0
        background-image: linear-gradient(25deg, $blue 0%, $blue_alt3 100%)
        box-shadow: 0px 0px 30px rgba(59, 183, 242, 0.75)
        transition: .3s ease-in-out
    &:hover
        color: $white
        border: 1px solid transparent
        transition: .3s ease-in-out
        &::before
            opacity: 1
            transition: .3s ease-in-out

.basic_page
    #content
        padding: 40px 0 60px
        .top
            text-align: center
            h1
                brandon_bold(35px, true)
                color: $black
        .bottom
            width: 100%
            max-width: 75%
            margin: 0 auto
            padding-top: 40px
            p,
            a
                color: $black
                brandon_regular(16px, false)

#main_container
    position: relative
    &.fish
        .login_sign_up
            margin-top: 0
            #login
                max-width: 100%
                margin: 60px 0 30px
                padding: 0 20px
                .title
                    brandon_bold(22px, false)
        .default_modal
            #health_waiver_form
                &.overlay
                    &.alt_2
                        max-width: calc(100% - 40px)
                .modal_wrapper
                    .form_title
                        brandon_bold(22px, false)
                        &.alt
                            padding: 30px 20px 0
                    .modal_main_group
                        padding: 0 20px 30px
        .form_group
            label
                brandon_medium(12px, true)
            .form_flex_radio_alternate
                margin-bottom: 15px
                label
                    flex: 0 0 40%
                .radio_wrapper
                    flex: 0 0 40%
        .form_check
            label
                a
                    pointer-events: none
        .buy_rides
            &.inner
                background-image: url('/dotted-bg.png')
                #payments
                    padding: 30px 0
                    &.success
                        padding: 0
                    .step
                        .preview
                            .preview_actions
                                @media (max-width: 767px) and (min-width: 320px)
                                    justify-content: center
                                #paypal-button-container
                                    .paypal-buttons
                                        min-width: 400px
                                        max-width: 1200px
                        .wrapper
                            .right
                                .action_mobile
                                    margin-top: 30px
                                    display: flex
                                    flex-flow: row wrap
                                    align-items: center
                                    justify-content: space-between
                                    .back,
                                    .default_btn_img
                                        width: auto
                                        flex: 0 0 auto
                                    .default_btn_img
                                        padding: 0 10px
                        .preview_payment
                            .preview
                                .back_wrapper
                                    margin-top: 10px
                                    text-align: center
                                    .back
                                        display: inline-block
                                        width: auto
        .book_a_bike
            &.landing
                #content
                    .left
                        .filter
                            .wrapper
                                &.studio_filter
                                    .group
                                        label
                                            &::before
                                                content : unset
            &.inner
                background-image: url('/dotted-bg.png')
                #content
                    padding: 60px 0
                    .main_left
                        margin-bottom: 30px
                        .header
                            h2
                                flex: 0 0 auto
                        .content
                            ul
                                padding: 10px 0
                                display: flex
                                flex-flow: row wrap
                                justify-content: center
                                li
                                    margin-bottom: 0
                                    padding: 0 5px
                        .description
                            padding: 0 10px 30px
                            h3
                                brandon_regular(14px, false)
                            ul
                                li
                                    brandon_regular(14px, false)
                        .waitlisted
                            padding: 20px 10px
                            .label
                                brandon_bold(18px, false)
                            .user
                                margin-top: 10px
                    .main_right
                        .content
                            padding: 30px 0
                            .footer
                                .actions
                                    .next_wrapper
                                        .left
                                            padding: 10px 0
                                            .flex
                                                &.package
                                                    display: flex
                                                    flex-flow: row wrap
                                                    align-items: center
                                                    justify-content: space-between
                                                    padding-right: 0
                                                    .toggler
                                                        justify-content: flex-start
                                                        flex: 0 0 48%
                                                        &:last-child
                                                            margin-bottom: 15px
                                                        &.alt
                                                            justify-content: flex-end
                                                        .picker
                                                            margin-left: 0
                                                            brandon_bold(12px, false)
                                                    p
                                                        brandon_regular(12px, false)
                                                        &.margin
                                                            margin-left: 10px
                                        .right
                                            &.alt
                                                display: flex
                                                flex-flow: row wrap
                                                align-items: center
                                                justify-content: space-between
                                            .back,
                                            .default_btn
                                                width: auto
                                                flex: 0 0 auto
                    .step
                        &.form
                            .preview_book_form
                                padding: 0
                                .preview
                                    .preview_actions
                                        display: flex
                                        flex-flow: row wrap
                                        align-items: center
                                        justify-content: space-between
                                        .back,
                                        .default_btn
                                            width: auto
                                            flex: 0 0 auto
                    .back
                        brandon_medium(14px, true)
    /**
     * Pro Tip */
    #pro_tip
        position: fixed
        top: calc(0% + 81px)
        left: 0
        right: 0
        background-color: $white
        padding: 12px 30px
        border-bottom: 0.5px solid $border_alt3
        display: flex
        flex-flow: row wrap
        align-items: center
        justify-content: space-between
        z-index: 5
        .tip
            flex: 1 0 50%
            brandon_regular(16px, false)
            font-style: italic
            color: rgba(23, 23, 23, 0.54)
            letter-spacing: 0.24px
            line-height: normal
        .tip_btn
            flex: 0 0 auto
            brandon_medium(12px, true)
            color: $green
            letter-spacing: 0.18px
            margin-left: 20px
            cursor: pointer
    /**
     * Complete Profile */
    #complete_profile
        position: fixed
        top: calc(0% + 81px)
        left: 0
        right: 0
        background-color: $green
        padding: 12px 15px
        z-index: 5
        &.mobile
            height: calc(100% - 150px)
            overflow-y: scroll
            overflow-x: hidden
            padding: 0 10px
            &::-webkit-scrollbar
                width: 6px
                background-color: $white
            &::-webkit-scrollbar-track
                box-shadow: none
            &::-webkit-scrollbar-thumb
                background-color: $green
                border: 0px solid rgba(0, 0, 0, 0)
                background-clip: padding-box
                border-radius: 50px
        .profile_text
            brandon_medium(14px, false)
            color: $white
            line-height: normal
            display: flex
            flex-flow: row wrap
            align-items: center
            justify-content: center
            .complete
                text-decoration: underline
                cursor: pointer
            .wrapper
                display: flex
                flex-flow: row wrap
                align-items: center
                padding: 0 10px
                .rec
                    width: 20px
                    flex: 0 0 20px
                    height: 7px
                    background-color: rgba(255, 255, 255, 0.52)
                    margin-left: 2px
                    &:last-child
                        padding-left: 0px
                    &.active
                        background-color: $white
        .close
            position: absolute
            width: 30px
            height: 30px
            top: calc(0% + 5px)
            right: calc(0% + 5px)
            cursor: pointer
            &::before
                content: ''
                position: absolute
                top: 50%
                right: calc(0% + 14px)
                width: 2px
                height: 20px
                background-color: $white
                transform: translateY(-50%) rotate(45deg)
            &::after
                content: ''
                position: absolute
                top: 50%
                right: calc(0% + 14px)
                width: 2px
                height: 20px
                background-color: $white
                transform: translateY(-50%) rotate(-45deg)
    /**
     * Navbar */
    #header
        display: flex
        flex-flow: row wrap
        align-items: center
        position: fixed
        top: 0
        left: 0
        z-index: 6
        width: 100%
        background-color: transparent
        box-shadow: 0 3px 6px transparent
        border-bottom: 0.5px solid $border_alt3
        transition: .2s linear
        &.login
            border-bottom: 0.5px solid transparent
            box-shadow: 0 3px 6px rgba(0, 0, 0, 0.1)
            background-color: $white
            transition: .2s linear
            .navbar
