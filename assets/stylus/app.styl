        .main
            flex: 0 0 100%
            height: 100%
            transition: .3s ease-in-out
            .overlay
                .main_image
                    padding: 100px 80px
                    transition: .3s ease-in-out
        .thumb
            right: -20%
            opacity: 0
            visibility: hidden
            transition: all .3s ease-in-out
