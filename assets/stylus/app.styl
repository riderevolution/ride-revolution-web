                &::after
                    content: ''
                    position: absolute
                    z-index: 1
                    bottom: 0
                    left: 0
                    width: 100%
                    height: 100%
                    background-image: linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.64) 0%)
                    opacity: 0
                    visibility: hidden
                    transition: .3s ease-in-out
