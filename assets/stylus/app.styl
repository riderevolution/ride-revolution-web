    .swiper-button-next,
    .swiper-container-rtl .swiper-button-prev
        background-image: none
        right: calc(0% + 25px)
    .swiper-pagination
        left: 0
        right: 0
        bottom: calc(0% + 25px) !important
        .swiper-pagination-bullet
            margin: 0 2.5px
            background: rgba(255, 255, 255, 0.45)
            opacity: 1
            transition: .3s ease-in-out
            &.swiper-pagination-bullet-active
                background: $white
                transition: .3s ease-in-out
    &.alt
        padding: 30px 100px 50px
        .swiper-button-prev
            &::before
                background-image: url('/icons/review-prev.svg')
            &::after
                background-image: url('/icons/review-prev-active.svg')
        .swiper-button-next
            &::before
                background-image: url('/icons/review-next.svg')
            &::after
                background-image: url('/icons/review-next-active.svg')
        .swiper-button-prev,
        .swiper-container-rtl .swiper-button-next
            z-index: 5
        .swiper-button-next,
        .swiper-container-rtl .swiper-button-prev
            z-index: 5
        &::before
            content: ''
            position: absolute
            top: 0
            width: 100px
            left: 0
            height: 100%
            z-index: 2
            background-color: $white_alt
        &::after
            content: ''
            position: absolute
            top: 0
            width: 100px
            right: 0
            height: 100%
            z-index: 2
            background-color: $white_alt

/**
 * Default Banner */
#banner
    position: relative
    overflow: hidden
    h1
        font-size: 0
    .main_image
        width: 100%
        height: 100vh
        object-position: top
    .overlay_top
        position: absolute
        top: 0
        left: 0
        width: 100%
        z-index: 1
        .overlay_image
            width: 920px
            animation: slide_from_left 1.5s
        .overlay_child
            position: absolute
            top: 50%
            right: calc(50% - 25%)
            text-align: center
            transform: translateY(-50%) translateX(0px)
            cursor: pointer
            animation: slide_from_rightY 1.5s
            /**
             * SVG */
            #play_icon
                transform: scale(1)
                transition: .3s ease-in-out
                .rect
                    fill: none
                .play
                    fill: $green_alt
                    transition: .3s ease-in-out
                .border
                    fill: none
                    stroke: $white
                    stroke-miterlimit: 10
                    stroke-dasharray: 500
                    animation: dash 5s linear
                    transition: .3s ease-in-out
            .label
                brandon_medium(18px, true)
                color: $white
                margin-top: 10px
                transition: .3s ease-in-out
            &:hover
                #play_icon
                    transform: scale(1.1)
                    transition: .3s ease-in-out
                    .border
                        stroke: $green_alt
                        transition: .3s ease-in-out
                .label
                    color: $green_alt
                    text-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
                    transition: .3s ease-in-out
    .overlay_bottom
        display: flex
        flex-flow: row wrap
        align-items: center
        justify-content: flex-start
        position: absolute
        bottom: calc(0% + 125px)
        left: 0
        right: 0
        width: 100%
        max-width: 75%
        margin: 0 auto
        z-index: 1
        animation: slide_from_left 1.5s
        p
            flex: 0 0 55%
            brandon_regular(20px, false)
            color: $white
            line-height: 1.5
            margin-right: 80px
        .default_btn
            flex: 0 0 auto
    &::before
        content: ''
        position: absolute
        top: 0
        left: 0
        z-index: 0
        width: 100%
        height: 100%
        background-color: rgba(0, 0, 0, 0.4)

/**
 * Close Icon **/
.close_icon
    position: absolute
    width: 30px
    height: 30px
    top: calc(0% + 30px)
    right: calc(0% + 40px)
    &::before
        content: ''
        position: absolute
        top: 0
        right: calc(0% + 13px)
        width: 2px
        height: 30px
        background-color: $black
        transform: rotate(45deg)
    &::after
        content: ''
        position: absolute
        top: 0
        right: calc(0% + 13px)
        width: 2px
        height: 30px
        background-color: $black
        transform: rotate(-45deg)

/**
 * Default Loader **/
.loader_wrapper
    position: fixed
    z-index: 999
    .background
        position: fixed
        top: 0
        left: 0
        width: 100%
        height: 100%
        z-index: 2
        background-color: rgba(0, 0, 0, 0.5)
    .wrapper
        display: flex
        align-items: center
        position: fixed
        top: 50%
        left: 50%
        z-index: 3
        text-align: center
        background-image: linear-gradient(to right, $red, $orange)
        box-shadow: 0px 10px 25px rgba(0,0,0,0.15)
        padding: 0 40px
        border-radius: 5px
        transform: translate(-50%, -50%)
        .loader
            display: inline-block
            position: relative
            width: 64px
            height: 64px
            div
                display: inline-block
                position: absolute
                left: 6px
                width: 13px
                background-color: $white
                animation: loader 1.2s cubic-bezier(0, 0.5, 0.5, 1) infinite
                &:nth-child(1)
                    left: 6px
                    animation-delay: -0.20s
                &:nth-child(2)
                    left: 26px
                    animation-delay: -0.10s
                &:nth-child(3)
                    left: 45px
                    animation-delay: 0
        .loader_text
            sf_pro_regular(18px)
            color: $white
            margin-left: 10px

/**
 * Animation */
.fade-enter-active,
.fade-leave-active
    transition: all .3s ease-in-out

.fade-enter,
.fade-leave-to
    opacity: 0

.page-enter-active,
.page-leave-active
    transition: all .3s ease-in-out

.page-enter,
.page-leave-to
    opacity: 0

.slide-enter-active,
.slide-leave-active
    transition: all .3s ease-in-out

.slide-enter,
.slide-leave-to
    opacity: 0
    transform: translateY(-20px)

.fade_in
    animation: fade_in .3s

.fade_out
    animation: fade_out .3s

.slide_from_left
    animation: slide_from_left .3s

.slide_from_right
    animation: slide_from_right .3s

@keyframes fade_in
	from {
		opacity: 0
    }
	to {
		opacity: 1
    }

@keyframes fade_out
	from {
		opacity: 1
    }
	to {
		opacity: 0
    }

@keyframes slide_from_left
	from {
		opacity: 0
        transform: translateX(-100px)
    }
	to {
		opacity: 1
        transform: translateX(0px)
    }

@keyframes slide_from_right
	from {
		opacity: 0
        transform: translateX(100px)
    }
	to {
		opacity: 1
        transform: translateX(0px)
    }

@keyframes slide_from_rightY
	from {
		opacity: 0
        transform: translateY(-50%) translateX(100px)
    }
	to {
		opacity: 1
        transform: translateY(-50%) translateX(0px)
    }

@keyframes slide_from_bottom
	from {
		opacity: 0
        transform: translateY(100px)
    }
	to {
		opacity: 1
        transform: translateX(0px)
    }

@keyframes loader
    0% {
        top: 6px
        height: 51px
    }
    50%, 100% {
        top: 19px
        height: 26px
    }

@keyframes blink
    0% {
        opacity: 1
    }
    50%, 100% {
        opacity: 0.3
    }

@keyframes dash
    from {
        stroke-dashoffset: 500
    }
    to {
        stroke-dashoffset: 0
    }
