@import 'fonts'

$green = #26A48B
$green_alt = #78F7DE
$green_alt2 = #82BCB1

$blue = #00B1FF
$blue_alt = #7AD1F7
$blue_alt2 = #E3F7FF

$pink = #9E558B
$pink_alt = #FD649C

$red = #F13747
$red_alt = #FFE5E6

$white = #FFF
$white_alt = #FCFCFC

$black = #171717
$black_alt = #4E4E4E
$black_alt2 = #53565C

$gray = #A8A8A8
$gray_alt = #ABADB9
$gray_alt2 = #808080

$border = #DEDEDE
$border_alt = #EDEEF0
$border_alt2 = #9C9EAA
$border_alt3 = #A0A0A0
$border_alt4 = #CECECE

image_default =
    height: auto
    vertical-align: middle
    object-fit: cover
    -webkit-user-drag: none

html
    -webkit-text-size-adjust: none
    -webkit-font-smoothing: subpixel-antialiased
    backface-visibility: hidden

*,
*:before,
*:after
    user-select: none
    box-sizing: border-box
    margin: 0

body
    overflow-x: hidden
    &.no_click
        pointer-events: none
    &.no_scroll
        overflow-y: hidden

input,
input:before,
input:after,
textarea,
textarea:before,
textarea:after,
select,
select:before,
select:after
    user-select: initial

h1,
h2,
h3,
h4,
h5
    font-weight: normal

table
    width: 100%
    border-collapse: collapse

img
    width: auto
    max-width: 100%
    {image_default}
    transition: .3s ease-in-out

a
    text-decoration: none

svg
    font-family: Brandon-Bold

/**
 * Default Buttons */
.default_btn
    display: inline-block
    width: auto
    brandon_bold(16px, true)
    color: $white
    letter-spacing: .7px
    padding: 15px 45px
    background-color: transparent
    border: none
    outline: none
    background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
    box-shadow: 0px 0px 30px transparent
    transition: .3s ease-in-out
    &:hover
        box-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
        transition: .3s ease-in-out

.default_btn_wht
    display: inline-block
    width: auto
    brandon_bold(16px, true)
    color: $green
    letter-spacing: .7px
    padding: 15px 45px
    background-color: $white
    border: none
    outline: none
    box-shadow: 0px 0px 30px transparent
    transition: .3s ease-in-out
    &.mb
        margin-bottom: 15px
    &:hover
        box-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
        transition: .3s ease-in-out

.default_btn_out
    display: inline-block
    position: relative
    brandon_bold(16px, true)
    color: $green
    letter-spacing: .7px
    outline: none
    padding: 15px 45px
    background-color: transparent
    border: 1px solid $green_alt2
    transition: .3s ease-in-out
    span
        position: inherit
    &::before
        content: ''
        position: absolute
        z-index: 0
        top: 0
        left: 0
        right: 0
        height: 100%
        opacity: 0
        background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
        box-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
        transition: .3s ease-in-out
    &:hover
        color: $white
        border: 1px solid transparent
        transition: .3s ease-in-out
        &::before
            opacity: 1
            transition: .3s ease-in-out

#main_container
    position: relative
    /**
     * Navbar Side */
    #header
        display: flex
        flex-flow: row wrap
        align-items: center
        position: fixed
        top: 0
        left: 0
        z-index: 6
        width: 100%
        background-color: transparent
        box-shadow: 0 3px 6px transparent
        border-bottom: 0.5px solid $border_alt3
        transition: .2s linear
        &.instructor
            background-color: $white
            box-shadow: 0 3px 6px transparent
            border-bottom: 0.5px solid $border_alt3
            transition: .2s linear
            .navbar
                padding: 10px 30px
                transition: .2s linear
                .logo
                    .logo_title
                        color: $black
                        transition: .3s ease-in-out
                .nav_list
                    li
                        .nav_item
                            color: $black_alt
                            transition: .3s ease-in-out
                            &:hover
                                color: $green
                                text-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
                                transition: .3s ease-in-out
            .nav_burger
                .bar
                    background-color: $black_alt
                    transition: .3s ease-in-out
                .excess_line
                    background-color: $black_alt
                    transition: .3s ease-in-out
        &.sticky,
        &.not_front
            border-bottom: 0.5px solid transparent
            box-shadow: 0 3px 6px rgba(0, 0, 0, 0.1)
            background-color: $white
            transition: .2s linear
            .navbar
                padding: 10px 30px
                transition: .2s linear
                .logo
                    .logo_title
                        color: $black
                        transition: .3s ease-in-out
                .nav_list
                    li
                        .nav_item
                            color: $black_alt
                            transition: .3s ease-in-out
                            &:hover
                                color: $green
                                text-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
                                transition: .3s ease-in-out
            .nav_burger
                .bar
                    background-color: $black_alt
                    transition: .3s ease-in-out
                .excess_line
                    background-color: $black_alt
                    transition: .3s ease-in-out
        .navbar
            display: flex
            flex-flow: row wrap
            align-items: center
            justify-content: space-between
            flex: 0 0 95%
            border-right: 0.5px solid $border_alt3
            padding: 15px 30px
            transition: .2s linear
            .logo
                text-decoration: none
                display: flex
                flex-flow: row wrap
                align-items: center
                justify-content: flex-start
                transition: .3s ease-in-out
                img
                    width: 45px
                    {image_default}
                .logo_title
                    neutra_bold(22px, true)
                    line-height: 1.25
                    color: $white
                    letter-spacing: 5px
                    padding-left: 20px
                    transition: .3s ease-in-out
            .nav_list
                list-style: none
                padding: 0
                margin: 0
                display: flex
                flex-flow: row wrap
                align-items: center
                li
                    margin: 0 30px
                    .nav_item
                        position: relative
                        brandon_bold(16px, true)
                        color: $white
                        letter-spacing: .5px
                        transition: .3s ease-in-out
                        &::before
                            content: ''
                            position: absolute
                            top: 100%
                            left: 0
                            width: 0
                            height: 3px
                            z-index: 1
                            background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
                            opacity: 0
                            transition: .3s ease-in-out .3s
                        &::after
                            content: ''
                            position: absolute
                            top: 100%
                            left: calc(100% - 25%)
                            width: 0
                            height: 3px
                            z-index: 1
                            background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
                            opacity: 0
                            transition: .3s ease-in-out .05s
                        &:hover
                            color: $green_alt
                            text-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
                            transition: .3s ease-in-out
                            &::before
                                width: calc(100% - 30%)
                                opacity: 1
                                transition: .3s ease-in-out .05s
                            &::after
                                width: calc(0% + 25%)
                                opacity: 1
                                transition: .3s ease-in-out .3s
        .nav_burger
            flex: 1 0 auto
            margin: 0 22px
            position: relative
            cursor: pointer
            .bar
                width: 100%
                background-color: $white
                height: 2px
                margin-bottom: 7.5px
                transition: .3s ease-in-out
                &.bottom_line
                    width: calc(100% - 20px)
                    margin-bottom: 0
            .excess_line
                position: absolute
                bottom: 0
                right: 0
                width: 10px
                height: 2px
                background-color: $white
                transition: .3s ease-in-out
    #footer
        position: relative
        display: flex
        flex-flow: row wrap
        align-items: flex-start
        padding: 60px 120px
        border-top: 1px solid #EFEFEF
        .logo
            flex: 0 0 15%
        .footer_link
            flex: 0 0 20%
            h3
                brandon_medium(16px, true)
                color: $black
                margin-bottom: 20px
            ul
                list-style: none
                margin: 0
                padding: 0
                li
                    margin-bottom: 5px
                    .footer_item
                        position: relative
                        brandon_regular(16px, false)
                        color: $black
                        transition: .3s ease-in-out
                        &::before
                            content: ''
                            position: absolute
                            top: 100%
                            left: 0
                            width: 0
                            height: 3px
                            z-index: 1
                            background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
                            opacity: 0
                            transition: .3s ease-in-out .3s
                        &::after
                            content: ''
                            position: absolute
                            top: 100%
                            left: calc(100% - 25%)
                            width: 0
                            height: 3px
                            z-index: 1
                            background-image: linear-gradient(25deg, $green 0%, $green_alt 100%)
                            opacity: 0
                            transition: .3s ease-in-out .05s
                        &:hover
                            color: $green
                            text-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
                            transition: .3s ease-in-out
                            &::before
                                width: calc(100% - 30%)
                                opacity: 1
                                transition: .3s ease-in-out .05s
                            &::after
                                width: calc(0% + 25%)
                                opacity: 1
                                transition: .3s ease-in-out .3s
            &.alt
                flex: 1 0 30%
                p
                    brandon_regular(16px, false)
                    color: $black
                    a
                        brandon_bold(16px, false)
                        color: $black
                        transition: .3s ease-in-out
                        &:hover
                            color: $green
                            text-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
                            transition: .3s ease-in-out
            .social_links
                display: flex
                flex-flow: row wrap
                align-items: center
                margin-bottom: 40px
                h4
                    brandon_regular(16px, false)
                    color: $black
                a
                    position: relative
                    font-size: 0
                    width: 25px
                    height: 25px
                    margin: 0 15px
                .fb
                    &::before
                        content: ''
                        position: absolute
                        top: 0
                        left: 0
                        width: 100%
                        height: 100%
                        opacity: 1
                        background-image: url('/icons/fb-icon.svg')
                        background-repeat: no-repeat
                        background-size: contain
                        background-position: center
                        transition: .3s ease-in-out
                    &::after
                        content: ''
                        position: absolute
                        top: 0
                        left: 0
                        width: 100%
                        height: 100%
                        opacity: 0
                        background-image: url('/icons/fb-icon-active.svg')
                        background-repeat: no-repeat
                        background-size: contain
                        background-position: center
                        transition: .3s ease-in-out
                    &:hover
                        &::before
                            opacity: 0
                            transition: .3s ease-in-out
                        &::after
                            opacity: 1
                            transition: .3s ease-in-out
                .ig
                    &::before
                        content: ''
                        position: absolute
                        top: 0
                        left: 0
                        width: 100%
                        height: 100%
                        opacity: 1
                        background-image: url('/icons/ig-icon.svg')
                        background-repeat: no-repeat
                        background-size: contain
                        background-position: center
                        transition: .3s ease-in-out
                    &::after
                        content: ''
                        position: absolute
                        top: 0
                        left: 0
                        width: 100%
                        height: 100%
                        opacity: 0
                        background-image: url('/icons/ig-icon-active.svg')
                        background-repeat: no-repeat
                        background-size: contain
                        background-position: center
                        transition: .3s ease-in-out
                    &:hover
                        &::before
                            opacity: 0
                            transition: .3s ease-in-out
                        &::after
                            opacity: 1
                            transition: .3s ease-in-out
        .back_to_top
            cursor: pointer
            position: absolute
            top: 50%
            right: 0
            display: flex
            flex-flow: row wrap
            align-items: center
            transform: translateY(-50%) rotate(90deg)
            img
                transform: rotate(-90deg)
            span
                brandon_medium(14px, true)
                letter-spacing: 1.4px
                color: $black
                padding-left: 40px
                transition: .3s ease-in-out
                &:hover
                    color: $green
                    text-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
                    transition: .3s ease-in-out
    #navbar_expanded
        position: fixed
        top: 0
        left: 0
        z-index: 999
        width: 100%
        height: 100%
        background-color: $white
        display: flex
        flex-flow: row wrap
        align-items: flex-start
        &.sticky
            .close_icon
                top: calc(0% + 20px)
            .logo
                padding: 10px 30px
                transition: .2s linear
        .logo
            padding: 15px 30px
            text-decoration: none
            display: flex
            flex-flow: row wrap
            align-items: center
            justify-content: flex-start
            transition: .2s linear
            img
                width: 45px
                {image_default}
            .logo_title
                neutra_bold(22px, true)
                line-height: 1.25
                color: $black
                letter-spacing: 5px
                padding-left: 20px
                transition: .3s ease-in-out
        .content
            flex: 0 0 100%
            display: flex
            flex-flow: row wrap
            align-items: flex-start
            justify-content: space-between
            padding: 0 95px 40px
            .left
                flex: 0 0 45%
                animation: slide_from_left 1.5s
                .expanded_main_list
                    list-style: none
                    margin: 0
                    padding: 0
                    li
                        margin-bottom: 80px
                        .main_item
                            display: inline-block
                            position: relative
                            #menu
                                .letter
                                    fill: transparent
                                    stroke: $black
                                    transition: .3s ease-in-out
                                    for num in (1...15)
                                        &:nth-child({num})
                                            transition-delay: num * 50ms
                            &::before
                                content: ''
                                position: absolute
                                top: calc(0% - 20px)
                                right: calc(0% - 65px)
                                width: 40px
                                height: 82px
                                background-color: $white
                                transform: rotate(-25deg)
                                transition: .3s ease-in-out
                            &:hover
                                &::before
                                    right: calc(0% - 25px)
                                    transition: .3s ease-in-out
                                #menu
                                    .letter
                                        fill: $green
                                        stroke: $white
                                        transition: .3s ease-in-out
                                        for num in (1...15)
                                            &:nth-child({num})
                                                transition-delay: num * 50ms
                        &:last-child
