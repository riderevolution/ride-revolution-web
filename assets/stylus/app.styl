/**
 * Front */
.home
    background-color: $white_alt
    /**
     * Promos */
    #promos
        .promo_slide
            position: relative
            &.swiper-slide-active,
            &.swiper-slide-duplicate-active
                .overlay
                    animation: slide_from_rightY 1s
            .overlay
                position: absolute
                top: 50%
                left: calc(0% + 15%)
                width: 100%
                max-width: 450px
                z-index: 1
                text-align: center
                padding: 30px
                background-color: $white
                transform: translateY(-50%)
                box-shadow: 0px 0px 30px transparent
                transition: .3s ease-in-out
                .header_title
                    brandon_bold(14px, true)
                    color: $black_alt2
                    letter-spacing: 1.5px
                .title
                    brandon_bold(30px, false)
                    color: $black
                    margin: 20px 0 15px
                .description
                    brandon_regular(16px, false)
                    color: $black_alt2
                    line-height: 1.5
                .code
                    brandon_bold(30px, true)
                    color: $green
                    margin: 20px 0
                    background-color: transparent
                    text-align: center
                    border: none
                    outline: none
                    box-shadow: none
                    pointer-events: none
                &:hover
                    box-shadow: 0px 0px 30px rgba(0, 0, 0, 0.5)
                    transition: .3s ease-in-out
    /**
     * Packages */
    #packages
        .header
            display: flex
            flex-flow: row wrap
            align-items: center
            justify-content: space-between
            margin: 50px 0
            .image_bg
                font-size: 0
                flex: 0 0 50%
                background-image: url(/packages.svg)
                padding: 7% 0
                background-repeat: no-repeat
                background-position: left center
                background-size: contain
            .description
                flex: 0 0 40%
                margin-right: 50px
                text-align: right
                p
                    brandon_regular(20px, false)
                    letter-spacing: 0.5px
                    color: $black
                    text-align: left
                    margin-bottom: 40px
                    .red
                        color: $red
                .link
                    display: flex
                    flex-flow: row wrap
                    align-items: center
                    justify-content: flex-end
                    brandon_bold(20px, false)
                    letter-spacing: 0.5px
                    color: $green
                    transition: .3s ease-in-out
                    span
                        padding-right: 20px
                    div
                        margin-right: 4px
                        border-top: 5px solid transparent
                        border-left: 7px solid $green
                        border-bottom: 5px solid transparent
                        animation: blink 1s cubic-bezier(0, 0.5, 0.5, 1) infinite
                        &:nth-child(2)
                            animation-delay: -0.40s
                        &:nth-child(3)
                            animation-delay: -0.20s
                        &:nth-child(4)
                            margin-right: 0px
                            animation-delay: 0
                    &:hover
                        text-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
                        transition: .3s ease-in-out
        .content
            display: flex
            flex-flow: row wrap
            justify-content: center
            width: 100%
            max-width: calc(80% + 30px)
            margin: 0 auto 50px
            .package_wrapper
                position: relative
                display: flex
                flex-flow: column
                flex: 0 0 calc(33.33% - 30px)
                margin: 15px
                text-align: center
                background-color: $white
                padding: 50px 20px 20px
                box-shadow: 0px 10px 15px transparent
                overflow: hidden
                transform: perspective(1px) scale(1)
                transition: .3s ease-in-out
                .package_header
                    display: flex
                    flex-flow: row wrap
                    flex: auto
                    .title
                        flex: 0 0 100%
                        brandon_bold(36px, false)
                        color: $black
                        line-height: 1
                    .description
                        flex: 0 0 100%
                        margin: 30px 0 80px
                        brandon_medium(16px, true)
                        color: $black
                        line-height: 1.75
                .discounted_price
                    flex: 0 0 auto
                    brandon_regular(14px, false)
                    color: $black
                    text-decoration: line-through
                    margin-bottom: 5px
                .price
                    brandon_bold(20px, false)
                    color: $green
                    flex: 0 0 auto
                .expires
                    brandon_medium(14px, true)
                    color: $gray
                    flex: 0 0 auto
                    margin: 10px 0 20px
                .default_btn_wht
                    flex: 0 0 auto
                    width: 100%
                &.promo
                    .ribbon
                        position: absolute
                        top: calc(0% + 30px)
                        left: calc(0% - 60px)
                        width: 200px
                        brandon_bold(16px, true)
                        color: $white
                        background-color: $pink
                        transform: rotate(-45deg)
                        transition: .3s ease-in-out
                    .price
                        color: $pink
                &:hover
