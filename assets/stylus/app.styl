                        stroke: $white
                        stroke-linecap: round
                        stroke-width: 0.75px
                        stroke-miterlimit: 10
                        stroke-dashoffset: 500
                        stroke-dasharray: 500
                        transition: 1s ease-in-out
                .overlay
                    position: absolute
                    bottom: 0
                    left: 0
                    width: 100%
                    z-index: 2
                    padding: 20px
                    h2
                        brandon_bold(20px, true)
                        color: $white
                        letter-spacing: 1px
