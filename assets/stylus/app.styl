                &:hover
                    &::before
                        opacity: 0
                        visibility: hidden
                        transition: .3s ease-in-out
                    &::after
                        opacity: 1
                        visibility: visible
                        transition: .3s ease-in-out
                    .follow
                        top: calc(0% + 50px)
                        opacity: 1
                        visibility: visible
                        transition: .3s ease-in-out
                    #stripe
                        opacity: 1
                        visibility: visible
                        transition: .3s ease-in-out
