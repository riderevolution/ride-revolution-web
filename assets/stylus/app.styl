        max-width: 65%
        margin: 0 auto
        p
            brandon_regular(18px, false)
            letter-spacing: 0.9px
            color: $black
            line-height: 1.5
    #visit
        background-color: $black
        padding: 80px 0
        h2
            text-align: center
            brandon_bold(50px, true)
            color: $white
            letter-spacing: 2.5px
            margin-bottom: 25px
        .content
            &.desktop
                display: flex
                flex-flow: row wrap
                align-items: flex-start
                justify-content: center
                width: 100%
                max-width: 75%
                margin: 0 auto
                .wrapper
                    flex: 0 0 calc(33.33% - 20px)
                    margin: 0 10px
            .wrapper
                h2
                    brandon_bold(22px, true)
                    color: $white
                    margin-top: 20px
                    transition: .3s ease-in-out
                .image
                    position: relative
                    /**
                     * SVG */
                    #stripe
                        position: absolute
                        top: 0
                        left: 0
                        z-index: 2
                        opacity: 0
                        visibility: hidden
                        transition: .3s ease-in-out
                        .border
                            fill: none
                            stroke: $white
                            stroke-linecap: round
                            stroke-width: 0.75px
                            stroke-miterlimit: 10
                            stroke-dashoffset: 500
                            stroke-dasharray: 500
                            transition: 1s ease-in-out
                    .overlay
                        position: absolute
                        z-index: 2
                        bottom: 0
                        left: 0
                        width: 100%
                        padding: 0 20px 20px
                        opacity: 0
                        visibility: hidden
                        transition: .3s ease-in-out
                        .default_btn
                            width: 100%
                            text-align: center
                    &::before
                        content: ''
                        position: absolute
                        top: 0
                        left: 0
                        width: 100%
                        height: 100%
                        background-color: rgba(0, 0, 0, 0.6)
                        z-index: 1
                        opacity: 0
                        visibility: hidden
                        transition: .3s ease-in-out
                &:hover
                    h2
                        color: $green_alt
                        transition: .3s ease-in-out
                    .image
                        &::before
                            opacity: 1
                            visibility: visible
                            transition: .3s ease-in-out
                        #stripe
                            opacity: 1
                            visibility: visible
                            transition: .3s ease-in-out
                            .border
                                stroke-dashoffset: 0
                                transition: 3s ease-in-out
                        .overlay
                            opacity: 1
                            visibility: visible
                            transition: .3s ease-in-out
                            .default_btn
                                box-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
                                transition: .3s ease-in-out

