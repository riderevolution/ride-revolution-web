                    p
                        brandon_regular(16px, false)
                        color: $black
                        a
                            brandon_bold(16px, false)
                            color: $black
                            transition: .3s ease-in-out
                            &:hover
                                color: $green
                                text-shadow: 0px 0px 30px rgba(38, 164, 139, 0.75)
                                transition: .3s ease-in-out
                    .social_links
                        display: flex
                        flex-flow: row wrap
                        align-items: center
                        margin-top: 30px
                        h4
                            brandon_regular(16px, false)
                            color: $black
                        a
                            position: relative
                            font-size: 0
                            width: 25px
                            height: 25px
                            margin: 0 15px
                        .fb
                            &::before
                                content: ''
                                position: absolute
                                top: 0
                                left: 0
                                width: 100%
                                height: 100%
                                opacity: 1
                                background-image: url('/icons/fb-icon.svg')
                                background-repeat: no-repeat
                                background-size: contain
