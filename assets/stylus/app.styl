            position: absolute
            display: flex
            flex-flow: row wrap
            align-items: center
            justify-content: space-between
            width: 100%
            top: 50%
            transform: translateY(-50%)
            transition: .3s ease-in-out
            .left_arrow
                width: 38px
                height: 38px
                margin-left: 20px
                background-image: url('/icons/left-arrow.svg')
                background-position: center
                background-repeat: no-repeat
                background-color: $black
                border: none
                outline: none
                opacity: .75
                transition: .3s ease-in-out
                &:hover,
                &:focus
                    opacity: 1
                    transition: .3s ease-in-out
            .main_image
                text-align: center
                flex: 0 0 75%
                padding: 40px
                transition: .3s ease-in-out
                img
                    height: 750px
                .image_title
                    brandon_regular(16px, false)
                    color: $white
                    margin-top: 30px
            .right_arrow
                width: 38px
                height: 38px
                margin-right: 20px
                background-image: url('/icons/right-arrow.svg')
                background-position: center
                background-repeat: no-repeat
                background-color: $black
                border: none
                outline: none
                opacity: .75
                transition: .3s ease-in-out
                &:hover,
                &:focus
                    opacity: 1
                    transition: .3s ease-in-out
        .controls
            flex: 0 0 100%
            display: flex
            flex-flow: row wrap
            align-items: center
            justify-content: space-around
            position: absolute
            top: 0
            right: 10px
            width: 100px
            height: 40px
            background-color: rgba(20, 20, 20, 0.65)
            padding: 0 10px
            .control_toggle
                width: 15px
                height: 15px
                background-image: url('/icons/toggle.svg')
                background-position: center
                background-repeat: no-repeat
                background-size: contain
                cursor: pointer
                margin-bottom: 5px
                background-color: transparent
                border: none
                outline: none
            .control_close
                position: relative
                width: 22px
                height: 20px
                cursor: pointer
